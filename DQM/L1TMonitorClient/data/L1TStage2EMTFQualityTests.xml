<TESTSCONFIGURATION>

    <QTEST name="NoisyChannel_Chamber">
	<TYPE>NoisyChannel</TYPE>	
	<PARAM name="tolerance">.5</PARAM>
	<PARAM name="neighbours">0</PARAM>
	<PARAM name="error">0.50</PARAM>
	<PARAM name="warning">0.70</PARAM>
    </QTEST>

    <QTEST name="ContentSigma_Chamber">
	<TYPE>ContentSigma</TYPE>	
	<PARAM name="toleranceNoisy">1</PARAM>
	<PARAM name="toleranceDead">1</PARAM>
	<PARAM name="neighboursX">10</PARAM>
	<PARAM name="neighboursY">10</PARAM>
	<PARAM name="noisy">1</PARAM>
	<PARAM name="dead">1</PARAM>
	<PARAM name="error">0.30</PARAM>
	<PARAM name="warning">0.70</PARAM>
    </QTEST> 

    <QTEST name="XRange_Errors">
	<TYPE>ContentsXRange</TYPE>	
	<PARAM name="xmin">0</PARAM>
	<PARAM name="xmax">0</PARAM>
	<PARAM name="error">1</PARAM>
	<PARAM name="warning">1</PARAM>
    </QTEST>

    <QTEST name="NoisyChannel_TrackBX">
	<TYPE>NoisyChannel</TYPE>	
	<PARAM name="tolerance">1</PARAM>
	<PARAM name="neighbours">12</PARAM>
	<PARAM name="error">0.50</PARAM>
	<PARAM name="warning">0.70</PARAM>
    </QTEST>

    <QTEST name="NoisyChannel_TrackPhi">
	<TYPE>NoisyChannel</TYPE>	
	<PARAM name="tolerance">1</PARAM>
	<PARAM name="neighbours">128</PARAM>
	<PARAM name="error">0.50</PARAM>
	<PARAM name="warning">0.70</PARAM>
    </QTEST>

    <LINK name="*emtfChamberStrip_QT*">
        <TestName activate="true">NoisyChannel_Chamber</TestName>
    </LINK> 

    <LINK name="*emtfChamberStrip_QT*">
	<TestName activate="true">ContentSigma_Chamber</TestName>
    </LINK>

    <LINK name="*emtfErrors">
	<TestName activate="true">XRange_Errors</TestName>
    </LINK> 

    <LINK name="*emtfTrackBX_QT*">
        <TestName activate="true">NoisyChannel_TrackBX</TestName>
    </LINK>  

    <LINK name="*emtfTrackPhi_QT*">
        <TestName activate="true">NoisyChannel_TrackPhi</TestName>
    </LINK> 

</TESTSCONFIGURATION>
