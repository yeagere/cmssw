    <LINK name="*emtfErrors">
				<TestName activate="true">XRange_Errors</TestName>
    </LINK> 

    <LINK name="*emtfChamberStrip*">
        <TestName activate="true">ContentSigma_Noisy_128</TestName> 
        <TestName activate="true">ContentSigma_Dead_128</TestName>
        <TestName activate="true">ContentSigma_Noisy_160</TestName> 
        <TestName activate="true">ContentSigma_Dead_160</TestName>
